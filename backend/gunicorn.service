[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/www/yue/vue-django-project/vue3-company/backend
ExecStart=/root/.p38virtualenvs/vueDjango/bin/gunicorn --workers 3 --bind unix:/run/gunicorn.sock backend.wsgi:application

[Install]
WantedBy=multi-user.target